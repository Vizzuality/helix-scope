.c-map {
  height: 100%;
}

.leaflet-bar {
  border-radius: 10px;
  box-shadow: 0 0 0 rgba(0, 0, 0, 0.65);
}

.leaflet-touch .leaflet-bar {
  border: none;
}

.leaflet-top .leaflet-control {
  margin-top: 30px;
}

.leaflet-bar a {
  color: $color-main;
  border-radius: 20px !important;
  margin-right: 30px;
  margin-top: 10px;
}

.map {
  width: 100%;
  height: 100%;
}
